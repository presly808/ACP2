<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <listener>
        <listener-class>listener.MySessionListener</listener-class>
    </listener>

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>servlets.HelloServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>MySessionServlet</servlet-name>
        <servlet-class>servlets.MySessionServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ContexServlet</servlet-name>
        <servlet-class>servlets.ContextTestServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AdditionalServlet</servlet-name>
        <servlet-class>servlets.AdditionalTestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ContexServlet</servlet-name>
        <url-pattern>/context</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AdditionalServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MySessionServlet</servlet-name>
        <url-pattern>/count</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

</web-app>
